{
  "name": "rm3-react-controls",
  "version": "0.0.3",
  "description": "React tools for rm3",
  "main": "dist/index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --compilers js:babel-register tests/*.js",
    "karma": "./node_modules/.bin/karma start",
    "karma:travis": "./node_modules/.bin/karma start travis.conf.js",
    "travis": "npm run test && npm run karma:travis",
    "prepublish": "./node_modules/.bin/babel lib -d dist/",
    "spa": "./node_modules/.bin/browserify spa/bundle.js -o spa/bundle.out.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:rm3web/rm3-react-controls"
  },
  "browserify": {
    "transform": [
      [
        "browserify-jsx"
      ]
    ]
  },
  "author": "Wirehead <wh-npm@wirewd.com>",
  "license": "BSD-2-Clause",
  "dependencies": {
    "react": "~0.14.6",
    "react-addons-linked-state-mixin": "~0.14.6",
    "react-intl": "1.2.0",
    "sitepath": "^0.2.0"
  },
  "devDependencies": {
    "babel-cli": "~6.4.0",
    "babel-preset-react": "~6.3.13",
    "babel-register": "~6.3.13",
    "babelify": "~7.2.0",
    "browserify": "^13.0.0",
    "browserify-jsx": "~1.0.0",
    "chai": "~3.4.1",
    "intl": "^1.0.1",
    "istanbul": "~0.4.1",
    "karma": "~0.13.19",
    "karma-browserify": "~4.4.2",
    "karma-chrome-launcher": "~0.2.2",
    "karma-firefox-launcher": "~0.1.7",
    "karma-mocha": "~0.2.1",
    "karma-phantomjs-launcher": "~0.2.3",
    "karma-safari-launcher": "~0.1.1",
    "karma-sauce-launcher": "~0.3.0",
    "mocha": "~2.3.4",
    "phantomjs": "~1.9.19",
    "react-addons-test-utils": "~0.14.6",
    "react-dom": "~0.14.6"
  }
}
